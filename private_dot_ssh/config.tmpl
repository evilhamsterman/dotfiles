Host *
  {{ if (eq .chezmoi.os "darwin") }}
  UseKeychain yes
  {{ end }}
  AddKeysToAgent yes

# Old Switches
Host cs-5-dist-1.corp.qumulo.com
  User admin
  KexAlgorithms +diffie-hellman-group1-sha1
  Ciphers aes128-cbc
Host cs-14-dist-1.corp.qumulo.com
  User admin
  KexAlgorithms +diffie-hellman-group1-sha1
  Ciphers aes128-cbc
Host cs-16-dist-1.corp.qumulo.com
  User admin
  KexAlgorithms +diffie-hellman-group1-sha1
  Ciphers aes128-cbc
Host cfw-15e-05-ipmi.corp.qumulo.com
  User admin
  KexAlgorithms +diffie-hellman-group1-sha1
  Ciphers aes128-cbc

# Remote VM
Host dmills-remote
  HostName dmills-remote.corp.qumulo.com
  ForwardAgent yes
  User dmills
Host dmills-remote-cfw
  HostName dmills-remote-cfw.corp.qumulo.com
  ForwardAgent yes
  User dmills

# vCenter
Host it-vc*
  PreferredAuthentications password
  PubkeyAuthentication no

Host vc-poc.corp.qumulo.com
  PreferredAuthentications password
  PubkeyAuthentication no

# Qumulo devices
Host *.qumulo.com
  User dmills

# Home Devices
Host *.mills.haus
  User dan
